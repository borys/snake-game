{
  "name": "snake",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "old school snake game",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest",
    "check-eslint": "eslint",
    "check-types": "tsc --project ./jsconfig.json",
    "check-prettier": "prettier ./src --check",
    "write-prettier": "prettier ./src --write",
    "generate-coverage": "cross-env NODE_OPTIONS=\"$NODE_OPTIONS --experimental-vm-modules\" jest --coverage",
    "generate-jsdoc": "jsdoc src -r -R README.md -d docs",
    "prepare": "husky"
  },
  "keywords": [],
  "author": "Przemysław Białek",
  "license": "ISC",
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@jest/globals": "^29.7.0",
    "@types/jest": "^29.5.14",
    "cross-env": "^7.0.3",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest": "^28.9.0",
    "eslint-plugin-jsdoc": "^50.5.0",
    "globals": "^15.12.0",
    "husky": "^9.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jsdoc": "^4.0.4",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3",
    "vite": "^5.4.11"
  },
  "packageManager": "pnpm"
}
